					<key>COMMAND_STRING</key>
					<string>cd ~/Documents/
rm -fr tmp
mkdir tmp
cd tmp

# Copy the wdgt file to tmp
template_path='';
for f in "$@"
do
	if [ "$f" != "Text" ]; then
		echo "$f"
		template_path="${f##*/}";
    	cp -R "$f" .
	fi
done

cd SliderViewerTemplate.wdgt/images/
rm -f Icon
