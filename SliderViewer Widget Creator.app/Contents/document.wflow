					<string># Copy the images to tmp
images_directory='';
for f in "$@"
do
	if [ "$f" != "Text" ]; then
		echo "$f"
		images_directory="${f##*/}";
    	cd ~/Documents/tmp/*.wdgt
    	cp -R "$f"/ images/
	fi
done

# Rename the widget - the widget name will be the image folder name
cd ~/Documents/tmp/
mv *.wdgt "$images_directory".wdgt</string>
